{
  "manifest_version": 2,

  "name": "moodle course filter",
  "description": "filtering the courses shown on the main page by semester",
  "version": "1.0",
  "author": "Yosef Kaner",

  "content_scripts": [
    {
      "all_frames": true,
      "js": [ "filter.js" ],
      "matches": [ "*://*.haifa.ac.il/" ],
      "run_at": "document_end"
    }
  ],

    "permissions" :
    [
      "activeTab",
      "tabs"
    ]
}